<?xml version="1.0" encoding="utf-8"?>
<productinfo version="3.0">
   <name>ARM Cortex-A9 (QEMU)</name>
   <pilinfo>
      <Name>Ethernet PIL</Name>
      <Transport IPAddress="10.10.10.1" Port="22">
         <Type>tcp/ip</Type>
         <Name>Ethernet PIL</Name>
         <IOInterfaceName>TCP/IP</IOInterfaceName>
     </Transport>
 </pilinfo>
 <token Name="ARM_CORTEX_A_BASE_ROOT_DIR" Value="codertarget.arm_cortex_a_base.internal.getSpPkgRootDir"/>
 <token Name="ARM_CORTEX_A_ROOT_DIR" Value="codertarget.arm_cortex_a.internal.getSpPkgRootDir"/>
 <token Name="BOOST_LIB" Value="codertarget.arm_cortex_a.internal.getBoostLibDir"/>
 <token Value="codertarget.arm_cortex_a.internal.getLinaroDir">
     <Name>Linaro Toolchain v4.8</Name>
   </token>
   <token Name="ESB_TARGET_ROOT" Value="soc.arm_cortex_a.internal.getRootFolder"/>
   <token Name="SDL" Value="codertarget.arm_cortex_a.internal.getSDLDir"/>
   <token Value="codertarget.arm_cortex_a_crl.internal.getNE10Dir">
      <Name>Ne10 Library</Name>
   </token>
   <onhardwareselecthook>codertarget.arm_cortex_a.internal.onHardwareSelect</onhardwareselecthook>
   <onaftercodegenhook>codertarget.arm_cortex_a.internal.onAfterCodeGen</onaftercodegenhook>
   <profiler BufferName="profilingData" DataLength="400" GetDataFcn="codertarget.arm_cortex_a_base.internal.getProfileData" InstantPrint="0" PrintData="1" StoreCoreId="1" TimerDataType="uint32" TimerReadFcn="profileReadTimer" TimerTicksPerS="1000000000" TimerUpcounting="1">
      <Name>ARM Cortex-A9 (QEMU) Profiler</Name>
      <TimerIncludeFile>$(ARM_CORTEX_A_BASE_ROOT_DIR)/include/profile_timer.h</TimerIncludeFile>
      <TimerSrcFile>$(ARM_CORTEX_A_BASE_ROOT_DIR)/src/profile_timer.c</TimerSrcFile>
   </profiler>
   <honorruntimestoprequest>true</honorruntimestoprequest>
   <includestdio>true</includestdio>
   <enableoneclick>true</enableoneclick>
   <externalmodeinfo closefcn="" connectfcn="" setupfcn="">
      <name>Ethernet External mode</name>
      <buildconfigurationinfofile>$(TARGET_ROOT)/registry/attributes/EthernetExternalmode_ExternalModeBuildConfig.xml</buildconfigurationinfofile>
      <preconnectfcn>pause(5);</preconnectfcn>
      <codertargetparameter callback="" name="Runtime.BuildAction">
         <value>Build, load, and run</value>
      </codertargetparameter>
      <transport name="tcpip">
         <ipaddress value="127.0.0.1">
            <visible>false</visible>
         </ipaddress>
         <port value="17725">
            <visible>false</visible>
         </port>
         <verbose>
            <value>false</value>
            <visible>true</visible>
         </verbose>
         <type>tcp/ip</type>
         <iointerfacename>TCP/IP</iointerfacename>
      </transport>
      <task default="background">
         <inbackground>true</inbackground>
         <inforeground>true</inforeground>
         <visible>true</visible>
      </task>
   </externalmodeinfo>
   <targetservice>
      <iointerfacename>TCP/IP</iointerfacename>
      <applicationservice linkflags="" name="ToAsyncQueueAppSvc">
         <library>$(TARGET_ROOT)/bin/libcodertarget_ToAsyncQueueTgtAppSvc.lib</library>
      </applicationservice>
      <applicationservice linkflags="" name="StreamingProfilerAppSvc">
         <library>$(ESB_TARGET_ROOT)/bin/libcodertarget_StreamingProfilerAppSvc.lib</library>
      </applicationservice>
      <buildconfigurationinfofile>$(TARGET_ROOT)/registry/attributes/TargetServiceBuildConfig.xml</buildconfigurationinfofile>
   </targetservice>
   <externalmodeinfo connectfcn="" name="">
      <buildconfigurationinfofile>$(TARGET_ROOT)/registry/attributes/TCPIPviaXCP_ExternalModeBuildConfig.xml</buildconfigurationinfofile>
      <preconnectfcn>pause(0)</preconnectfcn>
      <modelparameter callback="" name="OnTargetWaitForStart" value="on"/>
      <codertargetparameter callback="" name="Runtime.BuildAction">
         <value>Build, load, and run</value>
      </codertargetparameter>
      <transport>
         <ipaddress value="127.0.0.1">
            <visible>false</visible>
         </ipaddress>
         <port value="17725">
            <visible>false</visible>
         </port>
         <verbose>
            <value>false</value>
            <visible>true</visible>
         </verbose>
         <name>XCP on TCP/IP</name>
         <type>tcp/ip</type>
         <iointerfacename>XCP on TCP/IP</iointerfacename>
      </transport>
      <setupfcn>codertarget.utils.toggleXCPfeatures('on')</setupfcn>
      <closefcn>codertarget.utils.toggleXCPfeatures('off')</closefcn>
      <task default="background">
         <inbackground>true</inbackground>
         <inforeground>false</inforeground>
         <visible>false</visible>
      </task>
   </externalmodeinfo>
   <buildconfigurationinfofile>$(TARGET_ROOT)/registry/attributes/Target_BuildConfig_Linaro.xml</buildconfigurationinfofile>
</productinfo>